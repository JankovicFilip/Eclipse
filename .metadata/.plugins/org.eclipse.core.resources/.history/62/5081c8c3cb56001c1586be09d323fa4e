package edunova;

public class Pozdrav {
	/**
	 * ovo je dokumentacija - poslije
	 * @param args
	 */
	public static void main(String[] args) {
		// jedna linija komentar
		
		/*
		 *  ovo je vise 
		 *  linija
		 *  koda
		 *  komentar
		 */

		System.out.println(".-----------------------------------------------------------------------------.\r\n"
				+ "||Es| |F1 |F2 |F3 |F4 |F5 | |F6 |F7 |F8 |F9 |F10|                  C= AMIGA   |\r\n"
				+ "||__| |___|___|___|___|___| |___|___|___|___|___|                             |\r\n"
				+ "| _____________________________________________     ________    ___________   |\r\n"
				+ "||~  |! |\" |§ |$ |% |& |/ |( |) |= |? |` || |<-|   |Del|Help|  |{ |} |/ |* |  |\r\n"
				+ "||`__|1_|2_|3_|4_|5_|6_|7_|8_|9_|0_|ß_|´_|\\_|__|   |___|____|  |[ |]_|__|__|  |\r\n"
				+ "||<-  |Q |W |E |R |T |Z |U |I |O |P |Ü |* |   ||               |7 |8 |9 |- |  |\r\n"
				+ "||->__|__|__|__|__|__|__|__|__|__|__|__|+_|_  ||               |__|__|__|__|  |\r\n"
				+ "||Ctr|oC|A |S |D |F |G |H |J |K |L |Ö |Ä |^ |<'|               |4 |5 |6 |+ |  |\r\n"
				+ "||___|_L|__|__|__|__|__|__|__|__|__|__|__|#_|__|       __      |__|__|__|__|  |\r\n"
				+ "||^    |> |Y |X |C |V |B |N |M |; |: |_ |^     |      |A |     |1 |2 |3 |E |  |\r\n"
				+ "||_____|<_|__|__|__|__|__|__|__|,_|._|-_|______|    __||_|__   |__|__|__|n |  |\r\n"
				+ "|   |Alt|A  |                       |A  |Alt|      |<-|| |->|  |0    |. |t |  |\r\n"
				+ "|   |___|___|_______________________|___|___|      |__|V_|__|  |_____|__|e_|  |\r\n"
				+ "|                                                                             |\r\n"
				+ "`-----------------------------------------------------------------------------'");
	}

}
